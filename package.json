{
  "name": "earthui",
  "version": "0.1.0",
  "author": "Kimi Gao",
  "description": "a minimalism-style UI component library powered by react.js",
  "license": "MIT",
  "main": "dist/earth-ui.js",
  "module": "src/components/index.js",
  "engines": {
    "node": "^8.9.0",
    "npm": "^5.2.0"
  },
  "scripts": {
    "dist": "rimraf dist && webpack --progress --config config/webpack.config.js",
    "site:build": "NODE_ENV=production webpack --progress --config config/webpack.config.site.js",
    "site:dev": "webpack --progress --config config/webpack.config.site.js && node-dev site/server.js",
    "presite:prod": "npm i && NODE_ENV=production webpack --progress --config config/webpack.config.site.js",
    "site:prod": "NODE_ENV=production pm2 start site/server.js",
    "create": "node bin/create.js",
    "banner": "file-banner src --ignore '__tests__|\\.png'",
    "lint": "eslint src && eslint site",
    "fix": "eslint src --fix eslint site --fix",
    "test": "jest --config=config/jest.config.json",
    "coverage": "coveralls",
    "commit": "git-cz",
    "release": "standard-version"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "postcommit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "prop-types": "^15.6.1"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.6.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.22.0",
    "classlist": "^1.0.1",
    "classnames": "^2.2.5",
    "compression": "^1.7.2",
    "css-loader": "^0.28.7",
    "express": "^4.16.2",
    "file-loader": "^1.1.10",
    "invariant": "^2.2.2",
    "jest": "^22.4.2",
    "jsx-loader": "^0.13.2",
    "justified-layout": "^2.1.1",
    "less": "^2.7.3",
    "less-loader": "^3.0.0",
    "lodash": "^4.17.4",
    "multiparty": "^4.1.3",
    "nprogress": "^0.2.0",
    "pm2": "^2.10.1",
    "postcss-loader": "^2.0.8",
    "prop-types": "^15.6.1",
    "react": "^15.0.0",
    "react-avatar-editor": "^10.3.0",
    "react-css-modules": "^4.2.0",
    "react-dom": "^15.0.0",
    "react-flexbox-grid": "^2.0.0",
    "react-grid-gallery": "^0.4.8",
    "react-router": "^2.8.1",
    "react-update": "^0.4.4",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-merge": "^4.1.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-jest": "^10.0.0",
    "code-beautify": "0.0.11",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.18.1",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-loader": "^0.5.5",
    "husky": "^0.15.0-rc.8",
    "jest-css-modules": "^1.1.0",
    "jest-environment-jsdom-global": "^1.0.3",
    "lint-staged": "^7.0.0",
    "markdown-loader": "^2.0.2",
    "marked": "^0.3.17",
    "node-dev": "^3.1.3",
    "react-hot-loader": "^4.0.0-rc.0",
    "rimraf": "^2.6.2",
    "standard": "^11.0.0",
    "standard-version": "^4.3.0",
    "webpack-dashboard": "^1.1.1",
    "webpack-dev-middleware": "^2.0.6",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-livereload-plugin": "^1.0.0"
  }
}
